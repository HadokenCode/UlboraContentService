{
  "version": "0.1.0",
  "command": "go",
  "isShellCommand": true,
  "showOutput": "always",  
  "tasks": [
    {
      "taskName": "install",
      "suppressTaskName": false,
      "isBuildCommand": true,
      "args": [  
        "github.com/Ulbora/go-crud-mysql",     
